{
    "name":        "tuck/converter-bundle",
    "type":        "symfony-bundle",
    "description": "Adds a command for converting service container configs to other file formats",
    "keywords":    ["container", "convert"],
    "license":     "MIT",
    "authors": [
        {
            "name": "Ross Tuck",
            "homepage": "http://rosstuck.com"
        }
    ],
    "require": {
        "php":  "^5.5.9|>=7.0.8",
        "symfony/console": "^3.4",
        "symfony/yaml": "^3.4",
        "symfony/framework-bundle": "^3.4"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-0": {
            "Tuck\\ConverterBundle": ""
        }
    },
    "target-dir": "Tuck/ConverterBundle/"
}
